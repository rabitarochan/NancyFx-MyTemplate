@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<dynamic>
<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <link href="~/assets/bootstrap/css/bootstrap.css" rel="stylesheet"/>
    <link href="~/assets/bootstrap/css/bootstrap-responsive.css" rel="stylesheet"/>
    <link href="~/assets/app/css/app.css" rel="stylesheet"/>
    <script src="~/assets/common/js/jquery-1.10.2.js" type="text/javascript"></script>
    <script src="~/assets/bootstrap/js/bootstrap.js" type="text/javascript"></script>
    <script src="~/assets/backbone/js/underscore.js" type="text/javascript"></script>
    <script src="~/assets/backbone/js/json2.js" type="text/javascript"></script>
    <script src="~/assets/backbone/js/backbone.js" type="text/javascript"></script>
  </head>
  <body>
    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="brand" href="~/">Nancy App</a>
          <div class="nav-collapse collapse">
            <ul class="nav">
              <li><a href="#">Menu 01</a></li>
              <li><a href="#">Menu 02</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
    @RenderBody()
    @RenderSection("scripts", required: false)
  </body>
</html>
